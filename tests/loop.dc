FIVE := 0.0;
TWENTYFIVE := 0.0;

LOOP OUTER:
    FIVE := 0.0;
    LOOP INNER:
        IF FIVE >= 5 THEN
            BREAK
        ELSE
            FIVE := FIVE + 1
        FI
    REPEAT;
    TWENTYFIVE := TWENTYFIVE + FIVE;
    IF TWENTYFIVE >= 25 THEN
        BREAK
    FI
REPEAT;

PRINT FIVE;
PRINT TWENTYFIVE;
.
